@{
    ViewData["Title"] = "Всі товари";
}
@model IEnumerable<Store.Models.ProductModel>

<div class="container">
    <h1>Всі товари</h1>
    <div class="row">

        @foreach (var product in Model)
        {
            <div class="col-md-3">
                <partial name="_productPresentation" model="@product" />
            </div>
        }
    </div>
</div>